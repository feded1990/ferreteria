	<parameter name="FECHADESDE" class="java.util.Date"/>
	<parameter name="FECHAHASTA" class="java.util.Date"/>
	<queryString>
		<![CDATA[SELECT
     compras.`Id_Compra` AS compras_Id_Compra,
     compras.`Fecha` AS compras_Fecha,
     compras.`Total` AS compras_Total,
     clientes.`DNI` AS clientes_DNI,
     clientes.`Nombre` AS clientes_Nombre,
     clientes.`Apellido` AS clientes_Apellido
FROM
     `clientes` clientes INNER JOIN `compras` compras ON clientes.`DNI` = compras.`Id_Cliente`
WHERE
compras.Fecha <= $P{FECHAHASTA} AND compras.Fecha >= $P{FECHADESDE}
ORDER BY
compras_Fecha]]>
	</queryString>
